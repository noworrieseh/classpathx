	touch nntp.ok

mbox.ok:
@ENABLE_MBOX_TRUE@	grep "protocol=mbox" javamail.providers.in >> javamail.providers
	touch mbox.ok

maildir.ok:
@ENABLE_MAILDIR_TRUE@	grep "protocol=maildir" javamail.providers.in >> javamail.providers
	touch maildir.ok

META-INF/javamail.default.address.map: META-INF javamail.address.map
	cp javamail.address.map META-INF/javamail.default.address.map

